@model FormCollection
@{
    ViewBag.Title = "Enquiry Sent";
}
<div class="page-title-container">
    <div class="container">
        <div class="page-title pull-left">
            <h2 class="entry-title">Enquiry Sent</h2>
        </div>
        <ul class="breadcrumbs pull-right">
            <li><a href="@Url.Content("~")">HOME</a></li>
            <li class="active">Enquiry</li>
        </ul>
    </div>
</div>

<section id="content">
    <div class="container">
        <div id="main">
            <div class="travelo-box">
                <div class="contact-form">
                    <h2>Thank you! <small>Your enquiry has been submitted and you will be contacted shortly.</small></h2>
                    <a class="button yellow full-width uppercase btn-small" href="@Url.Content("~")">Browse for other packages</a>
                </div>
            </div>
        </div>
    </div>
</section>